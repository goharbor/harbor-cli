.nh
.TH "HARBOR" "1"  "Harbor Community" "Harbor User Manuals"

.SH NAME
harbor-usergroup - Manage usergroup


.SH SYNOPSIS
\fBharbor usergroup [flags]\fP


.SH DESCRIPTION
Manage usergroup in Harbor


.SH OPTIONS
\fB-h\fP, \fB--help\fP[=false]
	help for usergroup


.SH OPTIONS INHERITED FROM PARENT COMMANDS
\fB-c\fP, \fB--config\fP=""
	config file (default is $HOME/.config/harbor-cli/config.yaml)

.PP
\fB-o\fP, \fB--output-format\fP=""
	Output format. One of: json|yaml

.PP
\fB-v\fP, \fB--verbose\fP[=false]
	verbose output


.SH EXAMPLE
.EX
  harbor usergroup list
.EE


.SH SEE ALSO
\fBharbor(1)\fP, \fBharbor-usergroup-create(1)\fP, \fBharbor-usergroup-delete(1)\fP, \fBharbor-usergroup-list(1)\fP, \fBharbor-usergroup-update(1)\fP