.nh
.TH "HARBOR" "1"  "Harbor Community" "Harbor User Manuals"

.SH NAME
harbor-gc - Manage Garbage Collection


.SH SYNOPSIS
\fBharbor gc [flags]\fP


.SH DESCRIPTION
Manage Garbage Collection in Harbor (schedule, history, logs)


.SH OPTIONS
\fB-h\fP, \fB--help\fP[=false]
	help for gc


.SH OPTIONS INHERITED FROM PARENT COMMANDS
\fB-c\fP, \fB--config\fP=""
	config file (default is $HOME/.config/harbor-cli/config.yaml)

.PP
\fB-o\fP, \fB--output-format\fP=""
	Output format. One of: json|yaml

.PP
\fB-v\fP, \fB--verbose\fP[=false]
	verbose output


.SH SEE ALSO
\fBharbor(1)\fP, \fBharbor-gc-list(1)\fP, \fBharbor-gc-log(1)\fP, \fBharbor-gc-run(1)\fP, \fBharbor-gc-schedule(1)\fP, \fBharbor-gc-update-schedule(1)\fP